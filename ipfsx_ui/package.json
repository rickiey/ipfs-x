    "@radix-ui/themes": "^3.1.6",
    "@suiware/kit": "^0.1.0",
    "@tanstack/react-query": "^5.62.7",
    "clsx": "^2.1.1",
    "ipfs-http-client": "^60.0.1",
    "lucide-react": "^0.468.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.4.1",
    "react-use": "^17.5.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
